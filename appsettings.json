{
    "Serilog": {
        "Using":  [ "Serilog.Sinks.BrowserConsole" ],
        "MinimumLevel": "Verbose",
        "WriteTo": [
            { "Name": "BrowserConsole" }
        ],
        "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
        "Properties": {
            "Application": "Sample"
        }
    },
    "CustomMessage":{
        "Message":"Hello world from appsettings.json!"
    }
}
